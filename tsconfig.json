{
	"compilerOptions": {
		"target": "es2016",
		"module": "esnext",
		"moduleResolution": "nodenext",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"outDir": "dist",
		"paths": {
			"@/*": ["./src/*"],
			"@routes/*": ["./src/routes/*"],
			"@controllers/*": ["./src/controllers/*"],
			"@middlewares/*": ["./src/middlewares/*"]
		}
	}
}
